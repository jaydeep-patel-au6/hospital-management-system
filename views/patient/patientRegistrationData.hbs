<table class="table">
  <thead>
    <tr>
      
      <th scope="col">File Number</th>
      <th scope="col">Patient Name</th>
      <th scope="col">Patient Age</th>
      <th scope="col">Gender</th>
      <th scope="col">Blood Group</th>
      <th scope="col">Contact Number</th>
      <th scope="col">Address</th>
      <th scope="col">Reffered doctor Name</th>
      <th scope="col">Emergency contact Name</th>
      <th scope="col">Relation with patient</th>
      <th scope="col">Emergency Contact Number</th>
      <th scope="col">Created At</th>
      
    </tr>
  </thead>
  <tbody>
    <tr>
        {{#each list}}
     
      <th>{{this.order}}</th>
      <td>{{this.patientName}}</td>
      <td>{{this.age}}</td>
      <td>{{this.gender}}</td>
      <td>{{this.bloodGroup}}</td>
      <td>{{this.contactNumber}}</td>
      <td>{{this.address}}</td>
      <td>{{this.refferedDoctor}}</td>
      <td>{{this.emergencyPersonName}}</td>
      <td>{{this.emergencyPationtRelation}}</td>
      <td>{{this.emergencyContactNumber}}</td>
      <td>{{this.createdAt}}</td>
     
      
      
    </tr>
        {{/each}}
  </tbody>
</table>