<a class="btn btn-secondary" href="/patientRegistration"><i class="fa fa-list-alt"></i>Patient Entry</a><br>
<h3>{{viewTitle}}</h3><br>
<table class="table">
  <thead>
    <tr>
      
      <th scope="col">File Number</th>
      <th scope="col">Patient Name</th>
      <th scope="col">Patient Age</th>
      <th scope="col">Gender</th>
      <th scope="col">Blood Group</th>
      <th scope="col">Contact Number</th>
      <th scope="col">Address</th>
      <th scope="col">Reffered doctor Name</th>
      <th scope="col">Emergency contact Name</th>
      <th scope="col">Relation with patient</th>
      <th scope="col">Emergency Contact Number</th>
      <th scope="col">Created At</th>
      <th scope="col">Update | Delete</th>
      <th scope="col">Insert Record</th>
      
    </tr>
  </thead>
  <tbody>
    <tr>
        {{#each list}}
     
      <th>{{this.order}}</th>
      <td>{{this.patientName}}</td>
      <td>{{this.age}}</td>
      <td>{{this.gender}}</td>
      <td>{{this.bloodGroup}}</td>
      <td>{{this.contactNumber}}</td>
      <td>{{this.address}}</td>
      <td>{{this.refferedDoctor}}</td>
      <td>{{this.emergencyPersonName}}</td>
      <td>{{this.emergencyPationtRelation}}</td>
      <td>{{this.emergencyContactNumber}}</td>
      <td>{{this.createdAt}}</td>
      <td>
         <a href="/patientRegistration/{{this._id}}"><i class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>|
          <a href="/patientRegistration/delete/{{this._id}}" onclick="return confirm('Are you sure to delete this record ?');"><i class="fa fa-trash fa-lg" aria-hidden="true"></i></a>
      </td>
      <td>
      <a href="#" class="btn btn-info btn-lg">
          <span class="glyphicon glyphicon-plus"></span> Book Appoinment and Insert Record
        </a>
      </td>
      
      
    </tr>
        {{/each}}
  </tbody>
</table>